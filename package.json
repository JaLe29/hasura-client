{
  "name": "hasura-gql-client",
  "version": "0.0.6",
  "keywords": [
    "graphql",
    "hasura",
    "hasura-gql-client",
    "hasura-client",
    "graphql-schema",
    "graphql-schema",
    "graphql-js",
    "graphql-api",
    "client",
    "api"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/JaLe29/hasura-gql-client.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && vite build --mode release && yarn build:types",
    "build:types": "tsc ./src/index.ts --declaration --emitDeclarationOnly --declarationDir ./dist/types",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "start:demo": "ts-node -r tsconfig-paths/register --project tsconfig.json --files ./example/example1.ts",
    "start:publish": "ts-node -r tsconfig-paths/register --project tsconfig.json --files ./scripts/lib-publish.ts"
  },
  "dependencies": {
    "axios": "^1.1.3"
  },
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.17.0",
    "@types/babel__core": "^7",
    "@types/node": "^16.11.36",
    "@types/prettier": "^2.6.1",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "~4.6.4",
    "vite": "^3.2.3"
  }
}
